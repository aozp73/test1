<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="shop.mtcoding.test1.model.BoardRepository">
    <select id="findByUserId" resultType="shop.mtcoding.test1.dto.BoardAllDto">
      select id, title, created_at from 
      (SELECT * FROM BOARD_TB where user_id = 1) 
      order by id desc
    </select>
</mapper>
